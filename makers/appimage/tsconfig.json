{
  "extends": "./node_modules/@tsconfig/node16-strictest/tsconfig.json",
  "compilerOptions": {
    "incremental": true,
    "tsBuildInfoFile": "./.tsbuildinfo.json",
    "module": "Node16",
    "moduleResolution": "Node16",
    "target": "ES2022",
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "outDir": "./dist",
    "declarationDir": "./types",
    "stripInternal": true
  },
  "files": [
    "./src/main.ts",
    "./src/utils.ts"
  ]
}
